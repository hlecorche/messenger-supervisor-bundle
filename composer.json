{
    "name": "ecommit/messenger-supervisor-bundle",
    "description": "Use symfony/messenger with Supervisor",
    "keywords": ["symfony", "messenger", "supervisor", "symfony-bundle"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "autoload": {
        "psr-4": { "Ecommit\\MessengerSupervisorBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ecommit\\MessengerSupervisorBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "require": {
        "php": "^8.2",
        "egulias/email-validator": "^3.0|^4.0",
        "lstrojny/fxmlrpc": "^0.22",
        "nyholm/psr7": "^1.0",
        "psr/http-client": "^1.0",
        "psr/log": "^1.1|^2|^3",
        "supervisorphp/supervisor": "^4.0|^5.0",
        "symfony/config": "^6.4|^7.4|^8.0",
        "symfony/console": "^6.4|^7.4|^8.0",
        "symfony/dependency-injection": "^6.4|^7.4|^8.0",
        "symfony/framework-bundle": "^6.4|^7.4|^8.0",
        "symfony/http-client": "^6.4|^7.4|^8.0",
        "symfony/http-kernel": "^6.4|^7.4|^8.0",
        "symfony/mailer": "^6.4|^7.4|^8.0",
        "symfony/messenger": "^6.4|^7.4|^8.0",
        "symfony/twig-bridge": "^6.4|^7.4|^8.0",
        "symfony/twig-bundle": "^6.4|^7.4|^8.0",
        "twig/twig": "^2.12.0|^3.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "doctrine/dbal": "^3.1|^4.0",
        "doctrine/doctrine-bundle": "^2.4.5|^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^10",
        "symfony/doctrine-messenger": "^6.4|^7.4|^8.0",
        "symfony/yaml": "^6.4|^7.4|^8.0",
        "vimeo/psalm": "^5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
